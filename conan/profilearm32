include(leosac_options)
[settings]
os=Linux
arch=armv7
compiler=gcc
compiler.version=10
compiler.libcxx=libstdc++11
cppstd=20
[options]
leosac:build_test=False
[build_requires]
[env]
CONAN_CMAKE_FIND_ROOT_PATH=/usr/arm-linux-gnueabi/
CHOST=arm-linux-gnueabi
CXX=arm-linux-gnueabi-g++-10
CC=arm-linux-gnueabi-gcc-10
RANLIB=arm-linux-gnueabi-ranlib
AR=arm-linux-gnueabi-ar
AS=arm-linux-gnueabi-as
libpq:ZIC=/usr/sbin/zic
